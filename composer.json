    "description": "accept payments",
    "keywords": [
        "browner12",
        "billing"
    ],
    "homepage": "https://github.com/browner12/billing",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Brown",
            "email": "browner12@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "~5.2",
        "stripe/stripe-php": "~3",
        "paypal/rest-api-sdk-php": "~1",
        "authorizenet/authorizenet": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "browner12\\billing\\": "src"
        }
